<ivysettings>
  <settings defaultResolver="chainResolver"/>
  <resolvers>
    <chain name="chainResolver" returnFirst="true">
      <!-- This resolver is primarily useful if you're working on maintaining the FudgeMsg-Dist download area before pushing changes. -->
      <!-- <url name="fudgemsg-org-dist-local">
        <ivy pattern="file://${user.home}/src/FudgeMsg-Dist/java/ivy/[organization]/[module]/[revision]/ivy.xml" />
        <artifact pattern="file://${user.home}/src/FudgeMsg-Dist/java/ivy/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
      </url> -->
      <url name="fudgemsg-org-dist">
        <ivy pattern="http://dist.fudgemsg.org/java/ivy/[organization]/[module]/[revision]/ivy.xml" />
        <artifact pattern="http://dist.fudgemsg.org/java/ivy/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
      </url>
    </chain>
  </resolvers>
</ivysettings>
